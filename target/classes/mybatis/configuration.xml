<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Mybatis 환경설정 -->
	<typeAliases>
		<typeAlias alias="ShopUser"	type="com.javaex.model.ShopUserVo"></typeAlias>

		<typeAlias alias="Notice" type="com.javaex.model.NoticeVo"></typeAlias>

		<typeAlias alias="Shop" type="com.javaex.model.ShopVo"></typeAlias>
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/mappers/ShopUser.xml" />
		<mapper resource="mybatis/mappers/Notice.xml" />
		<mapper resource="mybatis/mappers/Shop.xml" />
	</mappers>
</configuration>